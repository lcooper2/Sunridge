@page
@model Sunridge.Pages.Dashboard.AdminDash.Key.HistoryModel
@{
    ViewData["Title"] = "History";
}
<h3>Key History</h3>
<div class="border backgroundWhite container">
    <div class="row">
        <div class="col-12 text-right">
            <a asp-page="Upsert" class="btn btn-primary"><i class="fas fa-plus"></i>&nbsp; Create New</a>
        </div>
    </div>
    <br /> <br />
    <table id="DT_Load" class="table table-striped table-bordered" style="width:100%;">
        <thead>
            <tr>
                <th>Key</th>
                <th>Status</th>
                <th>Date Issued</th>
                <th>Date Returned</th>
                <th>Paid Amount</th>
                <th>Lot</th>
                <th></th>
            </tr>
        </thead>
    </table>


</div>
@section Scripts{
    <script src="~/js/keyHist.js"></script>
}
