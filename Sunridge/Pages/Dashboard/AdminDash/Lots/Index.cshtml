@page
@model Sunridge.Pages.Dashboard.AdminDash.Lots.IndexModel
@{
    ViewData["Title"] = "Lots";
    Layout = "_DashboardLayout";
}
<!--Lots Index-->
<h1>Lot Management</h1>
<div class="border backgroundWhite container">
    <div class="row">
        <div class="col-12 text-right">
            <a asp-page="Upsert" class="btn btn-primary"><i class="fas fa-plus"></i>&nbsp; Create New</a>
        </div>
    </div>
    <br /> <br />
    <table id="DT_Load" class="table table-striped table-bordered" style="width:100%;">
        <thead>
            <tr>
                <th>Lot</th>
                <th>Tax #</th>
                <th>Status</th>
                <th>Modified By</th>
                <th>Modified Date</th>
                <th></th>

            </tr>
        </thead>
    </table>
</div>
@section Scripts{
    <script src="~/js/lot.js"></script>
}

