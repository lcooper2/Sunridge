@page
@model Sunridge.Pages.Dashboard.AdminDash.Lots.HistoryModel
@{
    ViewData["Title"] = "History";
    Layout = "_DashboardLayout";
}
<h3>Lot History</h3>
<div class="border backgroundWhite container">
    <div class="row">
        <div class="col-12 text-right">
            <a asp-page="Upsert" class="btn btn-primary"><i class="fas fa-plus"></i>&nbsp; Create New</a>
        </div>
    </div>
    <br /> <br />
    <table id="DT_Load" class="table table-striped table-bordered" style="width:100%;">
        <thead>
            <tr>
                <th>Lot</th>
                <th>Owner</th>
                <th>is Primary</th>
                <th>Start Date</th>
                <th>End Date</th>
                <th></th>
            </tr>
        </thead>
    </table>


</div>
@section Scripts{
    <script src="~/js/lotHist.js"></script>
}
