@page
@model Sunridge.Pages.Dashboard.OwnerDash.LostAndFound.IndexModel
@{
    ViewData["Title"] = "Lost and Found";
    Layout = "_DashboardLayout";
}
<div class="border backgroundwhite container">
    <div class="row">
        <div class="col-6">
            <h2 class="text-primary">Lost and Found List</h2>
        </div>
        <div class="col-6 text-right">
            <a asp-page="Upsert" class="btn btn-primary"><i class="fas fa-plus"></i>&nbsp;Create New</a>
        </div>
    </div>
    <br />
    <br />
    <table id="DT_load" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>Item Name</th>
                <th>Date Listed</th>
                <th>Clamed</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts{
    <script src="~/js/lostandfound.js"></script>
}
